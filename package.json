{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest --watchAll",
    "citest": "jest --coverage"
  },
  "dependencies": {
    "@types/react-test-renderer": "^17.0.1",
    "axios": "^0.21.1",
    "contrast": "^1.0.1",
    "expo": "~42.0.1",
    "expo-localization": "~10.2.0",
    "expo-status-bar": "~1.0.4",
    "i18n-js": "^3.8.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-42.0.0.tar.gz",
    "react-native-status-bar-height": "^2.6.0",
    "react-native-web": "~0.13.12",
    "uuid-random": "^1.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@testing-library/react-native": "^7.2.0",
    "@types/i18n-js": "^3.8.2",
    "@types/jest": "^27.0.1",
    "@types/react": "~16.9.35",
    "@types/react-native": "~0.63.2",
    "jest-expo": "^42.1.0",
    "react-test-renderer": "^17.0.2",
    "typescript": "~4.0.0"
  },
  "jest": {
    "preset": "jest-expo",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{ts,tsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/jest.setup.js"
    ],
    "testMatch": [
      "**/*.test.ts?(x)"
    ]
  },
  "private": true
